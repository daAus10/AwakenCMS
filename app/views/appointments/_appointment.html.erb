<div id="<%= dom_id appointment %>">
  <p>
    <strong>Name:</strong>
    <%= appointment.name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= appointment.phone %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= appointment.date %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= appointment.email %>
  </p>

  <p>
    <strong>Special needs:</strong>
    <%= appointment.special_needs %>
  </p>

  <p>
    <strong>Employee:</strong>
    <%= appointment.employee_id %>
  </p>

</div>
